#fuzion_idiom340

# Define the string 's' as "example" 
# variable 'c' whose value depends on 's'
s := "example"
c :=

  if s.is_empty then       # Check if the string is empty
    "Empty string"         # If it's empty, set the value to "Empty string"
  else
    s.substring_codepoint s.codepoint_length-1   # If it's not empty, get the last character using codepoint indexing

# Print the value of the 'c'
say c